<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0"
          prefWidth="800.0"
          xmlns="http://javafx.com/javafx/16"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="org.goznak.panels.ConnectSettingsPanel">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="comLabel" text="Выберите COM-порт:" GridPane.halignment="RIGHT" />
      <ComboBox fx:id="comCombo" prefWidth="150.0" GridPane.columnIndex="1" />
      <Label fx:id="baudRateLabel" text="Выберите Baud rate:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
      <ComboBox fx:id="baudRateCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="connectButton" mnemonicParsing="false" text="Подлкючиться" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
      <Button fx:id="disconnectButton" mnemonicParsing="false" text="Отключиться" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   </children>
</GridPane>
